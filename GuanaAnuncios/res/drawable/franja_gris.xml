<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#C9C5C2" />
      <stroke
          android:width="1dp"
          android:color="#73716F" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#C9C5C2"
          android:endColor="#73716F"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#73716F" />
    </shape>
  </item>
</selector>